@model IEnumerable<Laboratorio.Models.Cadastro_Maquina>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Maquina</h1>

<p>
    <a asp-action="Create">Cadastro</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome_Maquina)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome_Maquina)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID_Maquina">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.ID_Maquina">Detalhe</a> |
                <a asp-action="Delete" asp-route-id="@item.ID_Maquina">Excluir</a>
            </td>
        </tr>
}
    </tbody>
</table>
